
{
  "version": 2,
  "builds": [
    {
      "src": "app.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/countries",
      "dest": "app.py"
    },
    {
      "src": "/time",
      "dest": "app.py"
    },
    {
      "src": "/update",
      "dest": "app.py"
    },
    {
      "src": "/(.*)",
      "dest": "/templates/index.html"
    }
  ]
}



